<%= link_to "Dashboard", dashboard_path, local:true %>
<% if current_user %>
  <%= link_to "Log Out", logout_path, local:true %>
<% else %>
  <%= link_to "Landing Page", root_path, local: true %>
  <%= link_to "Log In", login_path, local: true %>
<% end %>
<h1>Viewing Party</h1>
<ul>
<% if current_user %>
  <% @users.each do |user| %>
   <li> <%= user.email %></li>
  <% end %>
<% end %>
</ul>

<%= button_to "Create New User", '/register' , method: :get, local: true %>